      
        <%= form_with( url: login_path, local: true) do |f| %>
        <ul class="login-items">
          <li><h1>Sign in</h1></li>
          <li><%= f.label :username %></li>
          <li><%= f.text_field :username, class: 'form-control' %></li>
          <li><br></li>
          <li><%= f.submit "Log in", class: "btn btn-success" %><%= link_to "sign up?", signup_path, class: "sign-up-link" %></li>
        </ul>
        <% end %>
